{
  "name": "gpxs-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts && node script/copyDir.js",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "gen": "node ./script/genNewComp/index.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./",
    "prepare": "husky install"
  },
  "dependencies": {
    "prismjs": "^1.25.0",
    "vue": "^3.2.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.3",
    "inquirer": "^8.2.0",
    "less": "^4.1.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-md": "^0.11.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.28.10"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "npm run lint"
    ]
  }
}
